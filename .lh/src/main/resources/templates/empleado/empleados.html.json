            "content": "<!DOCTYPE html>\r\n<html lang=\"es\" xmlns:th=\"https://www.thymeleaf.org\">\r\n\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n</head>\r\n\r\n<body>\r\n    <div th:fragment=\"contenido\" class=\"container-fluid\">\r\n        <!-- AQUI VA LA TABLA -->\r\n        <div class=\"row\">\r\n            <div class=\"col-md-12\">\r\n                <!-- DATA TABLE -->\r\n                <!-- Pongo el breadcrumbs? -->\r\n                <h3 class=\"title-5 m-b-25\">Empleadoss</h3>\r\n                <div class=\"table-data__tool\">\r\n                    <div class=\"table-data__tool-left\">\r\n                        <!-- BUSCADOR DE EMPLEADO -->\r\n                        <!-- NAYELLY AGREGARA BUSCADOR -->\r\n                        <div class=\"rs-select2--light rs-select2--md\"></div>\r\n                    </div>\r\n                    <div class=\"table-data__tool-right\">\r\n                        <button class=\"au-btn au-btn-icon au-btn--green au-btn--small\">\r\n                            <i class=\"zmdi zmdi-plus\"></i>NUEVO EMPLEADO</button>\r\n                        <div class=\"rs-select2--dark rs-select2--sm rs-select2--dark2\">\r\n                            <select class=\"js-select2\" name=\"type\">\r\n                                <option selected=\"selected\">Exportar</option>\r\n                                <option value=\"\">EXCEL</option>\r\n                                <option value=\"\">PDF</option>\r\n                            </select>\r\n                            <div class=\"dropDownSelect2\"></div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div class=\"table-responsive table-responsive-data2\">\r\n                    <table class=\"table table-data2\">\r\n                        <thead>\r\n                            <tr>\r\n\r\n                                <th>Nombres</th>\r\n                                <th>Apellidos</th>\r\n                                <th>Direcci√≥n</th>\r\n                                <th>Correo</th>\r\n                                <th>Estado</th>\r\n                                <th>ROL</th>\r\n                                <th></th>\r\n                            </tr>\r\n                        </thead>\r\n                        <tbody>\r\n                            <tr th:each=\"empleado: ${empleadoList}\" class=\"tr-shadow\">\r\n                                <td th:text=\"${empleado.nombre}\"></td>\r\n                                <td th:text=\"${empleado.apellido}\"></td>\r\n                                <td th:text=\"${empleado.direccion}\" class=\"desc\"></td>\r\n                                <td>\r\n                                    <span th:text=\"${empleado.correo_empleado}\" class=\"block-email\"></span>\r\n                                </td>\r\n                                <td>\r\n                                    <!-- <span th:text=\"${empleado.estado_empleado}\" class=\"status--process\"></span> -->\r\n                                    <span th:text=\"${empleado.estado_empleado}\"\r\n                                        th:classappend=\"${empleado.estado_empleado == 'ACTIVO'} ? 'status--process' : 'status--denied'\"></span>\r\n                                </td>\r\n                                <td th:text=\"${empleado.empleado_fk}\"></td>\r\n                                <td>\r\n                                    <div class=\"table-data-feature\">\r\n                                        <button class=\"item\" data-toggle=\"tooltip\" data-placement=\"top\" title=\"Editar\"\r\n                                            th:href=\"@{/edit_empleados}\">\r\n                                            <i class=\"zmdi zmdi-edit\"></i>\r\n                                        </button>\r\n                                        <button class=\"item\" data-toggle=\"tooltip\" data-placement=\"top\"\r\n                     